    <script type="text/javascript">

    function getStartTime() 
    {
        var selectedHour = document.getElementById("startinghour").value;
        // var selectedHour = userHour.options[userHour.selectedIndex].value;
        var selectedMin= document.getElementById("startingmin").value;
        // var selectedMin = userMin.options[userMin.selectedIndex].value;
        var startingTime = selectedHour + ":" + selectedMin;
        // console.log(startingTime);
        return startingTime;
    }

    function gateTime() 
    {
        var gateHour = document.getElementById("gatehour").value;
        var gateMin = document.getElementById("gatemin").value;
        var gateTotal = gateHour + ":" + gateMin;
        // console.log(gateTotal);
        return gateTotal
    }

    function getWindowTime() 
    {
        var windowHour = document.getElementById("windowhour").value;
        var windowMin = document.getElementById("windowmin").value;
        var windowTotal = windowHour + ":" + windowMin;
        // console.log(windowTotal);
        return windowTotal
    }

    function getDockTime() 
    {
        var dockHour = document.getElementById("dockinhour").value;
        var dockMin = document.getElementById("dockinmin").value;
        var dockTimeTotal = dockHour + ":" + dockMin;
        // console.log(dockTimeTotal);
        return dockTimeTotal
    }

    function releasedFromDock () 
    {
        var releasedFromDockHour = document.getElementById("releaseddockhour").value;
        var releasedFromDockMin = document.getElementById("releaseddockmin").value;
        var totalReleasedTime = releasedFromDockHour + ":" + releasedFromDockMin;
        // console.log(totalReleasedTime)
        return totalReleasedTime
    }

    function receivedPaperwork ()
    {
        var receivedPaperHour = document.getElementById("receivedhour").value;
        var receivedPaperMin = document.getElementById("receivedmin").value;
        var totalPaperworkTime = receivedPaperHour + ":" + receivedPaperMin;
        // console.log(totalPaperworkTime)
        return totalPaperworkTime
    }

    function totalTime() 
    {   
        getStartTime();
        gateTime();
        getWindowTime();
        getDockTime();
        releasedFromDock();
        receivedPaperwork();
    }

    </script>


