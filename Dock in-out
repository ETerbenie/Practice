         <script type="application/javascript">
             
             function sendToTransflo() 
             {
                totalTime();

                var json = JSON.stringify(
							{
								"metafields": [
									{
										"id": "startinghour",
										"value": startingTime 
									},
									{
										"id": "gatehour",
										"value": gateTotal 
									},
									{
										"id": "windowhour",
										"value": windowTotal
									},
									{
										"id": "dockinhour",
										"value": dockTimeTotal
									},
									{
										"id": "releaseddockhour",
										"value": totalReleasedTime
									},
									{
										"id": "receivedhour",
										"value": totalPaperworkTime
									}
								]
							});
                alert(json);
                
                TRANSFLOWebFormListener.formSubmitted(json);
				document.getElementById('webform').submit();
             }
         </script>
